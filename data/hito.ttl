    a hito:FeatureClassified ;
    rdfs:comment "Manage Audit Trigger initiated to track system maintenance - EHR or clinical software."@en ;
    rdfs:label "System Maintenance -  EHR or Clinical Software System Audit Trigger"@en ;
    skos:definition "Capture system maintenance - EHR or clinical software, both routine and exceptional, including key metadata (who, what, when, where, why)."@en .

hito:EhrSfmSystemMaintenanceEventsLocalAccessSystemAuditTrigger
    hito:fClaFrom hito:EhrSfmFeatureCatalogue ;
