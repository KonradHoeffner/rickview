    hito:fClaFrom hito:EhrSfmFeatureCatalogue ;
    hito:internalId "TI.2.1.3.15" ;
    hito:subFeatureOf hito:EhrSfmSystemAuditTriggers ;
    a hito:FeatureClassified ;
    rdfs:comment "Manage Audit Trigger initiated to track system maintenance of codes, vocabulary, knowledge and rules."@en ;
    rdfs:label "System Maintenance -  Codes, Vocabulary, Knowledge, Rules  System Audit Trigger"@en ;
    skos:definition "Capture system maintenance of codes, vocabulary, knowledge and rules - both routine and exceptional, including key metadata (who, what, when, where, why)."@en .

